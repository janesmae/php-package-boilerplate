{
  "name": "janesmae/boilerplate",
  "description": "Boilerplate for PHP packages",
  "type": "vendor",
  "keywords": [
    "boilerplate"
  ],
  "version": "0.1.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Jaan Janesmae",
      "email": "jaan@janesmae.com",
      "homepage": "http://jaan.janesmae.com/",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/janesmae/php-package-boilerplate"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": {
      "janesmae\\boilerplate\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
